{
    "compilerOptions": {
      "target": "es5",
      "module": "ESNext",
      "strict": true,
      "esModuleInterop": true,
      "declaration": true,
      "declarationDir": "./types",
      "rootDir": ".",
      "baseUrl": ".",
      "moduleResolution": "node",
      "paths": {
        "#/*": ["./core/types/*"]
      }
    },
    "include": ["packages/**/*", "*/*"],
    "exclude": ["node_modules"]
  }